    "@types/react-modal": "^3.13.1",
    "@types/react-redux": "^7.1.24",
    "@types/redux-mock-store": "^1.0.3",
    "@types/redux-thunk": "^2.1.0",
    "axios": "^1.1.3",
    "axios-mock-adapter": "^1.21.2",
    "formik": "^2.2.9",
    "json-server": "^0.17.0",
