  "scripts": {
    "start": "react-scripts start",
    "json-server-auth": "json-server-auth --watch db.json --port 3333",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@shotgunjed)/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.stories.tsx",
      "!src/features/home/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*.style.tsx",
      "!src/tests/*",
      "!src/tests/**/*",
      "!src/assets/**/*",
      "!src/redux/store.ts",
      "!src/reportWebVitals.ts",
      "!src/serviceWorker.ts",
      "!src/index.tsx"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/react": "^13.4.0"
  }
